								<div class="carousel-inner" role="listbox">
								<div class="item active">
									<img
									src="/uploads/<%= project.thumbnail %>"
									class="d-block w-100 img-responsive"
									alt="<%= project.project_name %>"
									>
								</div>
								</div>

							</div>

							</a>
						</div>

