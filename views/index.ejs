						<div class="col-md-6">
							<div class="portfolio-des">
								<%
								const typeMap = {
									1: "Residential",
									2: "Commercial",
									3: "Villa",
									4: "School",
									5: "Interior"
								};
								%>
							<span class="color-theme"><%= typeMap[project.type] || "Architecture" %></span>
							<h3><%= project.project_name %></h3>
							<p>
								<%= project.description.length > 160
								? project.description.substring(0,160) + "..."
								: project.description
								%>
							</p>
							<a href="/single_project?id=<%= project.id %>" class="ot-btn btn-style-2">
								View Project
							</a>
							</div>
						</div>

						</div>
					<% }) %>

</div>

